{
	"realm": "${meveo.keycloak.realm}",
	"auth-server-url": "${meveo.keycloak.url:http://localhost:8080/auth}",
	"ssl-required": "${meveo.keycloak.ssl:none}",
	"resource": "${meveo.keycloak.client:meveo-web}",
	"credentials": {
		"secret": "${meveo.keycloak.secret}"
	},
	"use-resource-role-mappings": true,
	"enable-basic-auth": "${meveo.keycloak.basic-auth:true}",
	"principal-attribute": "preferred_username"
}
