/system-property=jboss.as.management.blocking.timeout:add(value=900)
/system-property=java.net.preferIPv4Stack:add(value=true)
/system-property=jboss.tx.node.id:add(value=meveo-default)
/system-property=meveo.admin.server.name:add(value=${env.MEVEO_DB_HOST:postgres})
/system-property=meveo.admin.port.number:add(value=${env.MEVEO_DB_PORT:5432})
/system-property=meveo.admin.database.name:add(value=${env.MEVEO_DB_NAME:meveo})
/system-property=meveo.admin.database.driver:add(value=postgresql)
/system-property=meveo.admin.database.username:add(value=${env.MEVEO_DB_USERNAME:meveo})
/system-property=meveo.admin.database.password:add(value=${env.MEVEO_DB_PASSWORD:meveo})
/system-property=meveo.admin.database.min-pool-size:add(value=${env.MEVEO_DB_MIN_POOL_SIZE:10})
/system-property=meveo.admin.database.max-pool-size:add(value=${env.MEVEO_DB_MAX_POOL_SIZE:200})
/system-property=meveo.keycloak.url:add(value=${env.KEYCLOAK_URL})
