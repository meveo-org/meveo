/system-property=resteasy.preferJacksonOverJsonB:add(value=true)
/system-property=odoo.admin.server.name:add(value=${env.ODOO_HOST:odoo})
/system-property=odoo.admin.port.number:add(value=${env.ODOO_DB_PORT:5432})
/system-property=odoo.admin.database.name:add(value=${env.ODOO_DB_NAME:odoo})
/system-property=odoo.admin.database.driver:add(value=postgresql)
/system-property=odoo.admin.database.username:add(value=${env.ODOO_DB_USERNAME:odoo})
/system-property=odoo.admin.database.password:add(value=${env.ODOO_DB_PASSWORD:odoo})
